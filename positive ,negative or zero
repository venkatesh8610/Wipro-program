// to find given number is positive or negative or zero
import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		//int b=sc.nextInt();
		if(a>0) System.out.print("The number is Positive");
		if(a==0) System.out.print("The number is Zero");
		if(a<0) System.out.print("The number is Negative");
	}
}
